<div style="background:white;" class="container">
<%= form_for :major, url: majors_path do |f| %>
    <p>
      <%= f.label :major %><br>
      <%= f.text_field :major %>
    </p>
   
    <p>
      <%= f.label :course %><br>
      <%= f.text_field :course %>
    </p>
   
    <p>
      <%= f.submit %>
    </p>
  <% end %>
</div>